{
    "extends": "@tsconfig/react-native/tsconfig.json", 
    "compilerOptions": {
      "outDir": "dist",
      "rootDir": "src",
      "sourceMap": true,
      "noEmit": false,
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "strictNullChecks": false,
      "skipLibCheck": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "allowSyntheticDefaultImports": true,
      "strict": true
    },
    "exclude": [
      "node_modules",
      "babel.config.js",
      "metro.config.js",
      "jest.config.js",
      "tsconfig.json",
      "rn-cli.config.js",
      "coverage",
      "android",
      "ios",
      "__tests__",
      "index.js",
      "dist"
      ]
}